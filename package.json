  "version": "3.0.68",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.1",
    "@material-ui/core": "^4.4.1",
    "apollo-boost": "^0.4.4",
    "axios": "^0.19.0",
    "cloudinary-react": "^1.2.1",
    "gatsby": "2.13.50",
    "gatsby-image": "^2.2.8",
    "gatsby-plugin-google-analytics": "^2.1.7",
    "gatsby-plugin-mailchimp": "^5.1.2",
    "gatsby-plugin-manifest": "2.2.4",
    "gatsby-plugin-offline": "2.2.4",
    "gatsby-plugin-postcss": "2.1.2",
    "gatsby-plugin-purgecss": "4.0.0",
    "gatsby-plugin-react-helmet": "3.1.3",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-sharp": "^2.2.11",
    "gatsby-plugin-typescript": "^2.1.6",
    "gatsby-source-filesystem": "^2.1.9",
    "gatsby-transformer-sharp": "^2.2.6",
    "graphql": "^14.5.8",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-dropzone": "^10.1.9",
    "react-helmet": "5.2.1",
    "tailwindcss-interaction-variants": "^2.2.0"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different '**/*.{js,css,md,json}'",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,json}'",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "babel-eslint": "10.0.2",
    "eslint": "6.1.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.6.1",
    "prettier": "1.18.2",
    "tailwindcss": "1.0.6",
    "typescript": "3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oddstronaut/gatsby-starter-tailwind"
  },
  "bugs": {
    "url": "https://github.com/oddstronaut/gatsby-starter-tailwind/issues"
  },
  "publishConfig": {
    "access": "public"
  }
